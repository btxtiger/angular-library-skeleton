{
   "name": "angular-library-skeleton",
   "version": "0.0.0",
   "private": true,
   "description": "Angular Library Skeleton Project",
   "keywords": [
      "Angular",
      "best-practice",
      "Angular Library"
   ],
   "homepage": "https://github.com/btxtiger/angular-library-skeleton#readme",
   "bugs": {
      "url": "https://github.com/btxtiger/angular-library-skeleton/issues"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/btxtiger/angular-library-skeleton.git"
   },
   "license": "MIT",
   "author": "Jonas Gnioui <btxtiger@icloud.com>",
   "scripts": {
      "build": "ng build",
      "build:demo": "ng build",
      "build:lib": "npm run build:lib:tw && ng build ngx-lib",
      "build:lib:tw": "tailwind --minify -c projects/ngx-lib/tailwind.config.js -i projects/ngx-lib/src/theme/tailwind.scss -o projects/ngx-lib/src/theme/tailwind-prod.scss",
      "bundle-report": "webpack-bundle-analyzer dist/ims-frontend/stats.json",
      "preinstall": "git config core.hooksPath hooks",
      "lint": "ng lint",
      "ng": "ng",
      "prettier-format": "prettier --log-level warn --write \"./{src,projects}/**/*.{ts,html,js,scss,json}\"",
      "publish": "npm run build:demo && npm run build:lib && cd dist/ngx-lib && npm publish",
      "start": "ng serve",
      "start:lazy": "ng serve --live-reload=false",
      "test": "ng test",
      "test:ci": "ng test --no-watch --no-progress --browsers=FirefoxHeadless",
      "watch": "ng build --watch --configuration development"
   },
   "dependencies": {
      "@angular/animations": "^16.1.8",
      "@angular/cdk": "^16.1.7",
      "@angular/common": "^16.1.8",
      "@angular/compiler": "^16.1.8",
      "@angular/core": "^16.1.8",
      "@angular/forms": "^16.1.8",
      "@angular/material": "^16.1.7",
      "@angular/platform-browser": "^16.1.8",
      "@angular/platform-browser-dynamic": "^16.1.8",
      "@angular/router": "^16.1.8",
      "@mdi/js": "^7.2.96",
      "@ngx-pwa/local-storage": "^16.3.0",
      "await-to-js": "^3.0.0",
      "change-case-all": "^1.0.15",
      "lodash-es": "^4.17.21",
      "mat-icon-button-sizes": "^1.0.4",
      "moment-mini": "github:ksloan/moment-mini",
      "ngx-md-icon": "^1.1.24",
      "ngx-modern-alerts": "^0.2.13",
      "prettier-plugin-jsdoc": "^1.0.1",
      "rxjs": "~7.8.0",
      "tailwindscss": "^0.3.0",
      "tslib": "^2.3.0",
      "zone.js": "~0.13.0"
   },
   "devDependencies": {
      "@angular-devkit/build-angular": "^16.1.7",
      "@angular-eslint/builder": "16.1.0",
      "@angular-eslint/eslint-plugin": "16.1.0",
      "@angular-eslint/eslint-plugin-template": "16.1.0",
      "@angular-eslint/schematics": "16.1.0",
      "@angular-eslint/template-parser": "16.1.0",
      "@angular/cli": "^16.1.7",
      "@angular/compiler-cli": "^16.1.8",
      "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
      "@types/jasmine": "~4.3.0",
      "@typescript-eslint/eslint-plugin": "^6.2.1",
      "@typescript-eslint/parser": "^6.2.1",
      "angular-best-practice": "github:btxtiger/angular-best-practice",
      "autoprefixer": "^10.4.14",
      "eslint": "^8.46.0",
      "eslint-plugin-decorator-position": "^5.0.2",
      "eslint-plugin-jsdoc": "^46.4.5",
      "jasmine-core": "^5.1.0",
      "karma": "~6.4.0",
      "karma-chrome-launcher": "~3.2.0",
      "karma-coverage": "~2.2.0",
      "karma-firefox-launcher": "^2.1.2",
      "karma-jasmine": "~5.1.0",
      "karma-jasmine-html-reporter": "~2.1.0",
      "ng-packagr": "^16.0.0",
      "postcss": "^8.4.27",
      "prettier": "^3.0.1",
      "prettier-plugin-organize-imports": "^3.2.3",
      "prettier-plugin-packagejson": "^2.4.5",
      "prettier-plugin-style-order": "^0.2.2",
      "prettier-plugin-tailwindcss": "^0.4.1",
      "tailwindcss": "^3.3.3",
      "typescript": "~5.1.3",
      "webpack-bundle-analyzer": "^4.9.0"
   }
}
